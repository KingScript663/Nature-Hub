local function performFirstPress(parryType)
Â Â Â  if parryType == 'F_Key' then
Â Â Â Â Â Â Â  VirtualInputService:SendKeyEvent(true, Enum.KeyCode.F, false, nil)
Â Â Â  elseif parryType == 'Left_Click' then
Â Â Â Â Â Â Â  VirtualInputManager:SendMouseButtonEvent(0, 0, 0, true, game, 0)
Â Â Â  elseif parryType == 'Navigation' then
Â Â Â Â Â Â Â  local button = Players.LocalPlayer.PlayerGui.Hotbar.Block
Â Â Â Â Â Â Â  updateNavigation(button)
Â Â Â Â Â Â Â  VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.Return, false, game)
Â Â Â Â Â Â Â  VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.Return, false, game)
Â Â Â Â Â Â Â  task.wait(0.01)
Â Â Â Â Â Â Â  updateNavigation(nil)
Â Â Â  end
end

