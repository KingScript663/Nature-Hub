local Closest_Entity = nil

function Auto_Parry.Closest_Player()
Â Â Â  local Max_Distance = math.huge
Â Â Â  local Found_Entity = nil
Â Â Â  
Â Â Â  for _, Entity in pairs(workspace.Alive:GetChildren()) do
Â Â Â Â Â Â Â  if tostring(Entity) ~= tostring(Player) then
Â Â Â Â Â Â Â Â Â Â Â  if Entity.PrimaryPart thenÂ  -- Check if PrimaryPart exists
Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  local Distance = Player:DistanceFromCharacter(Entity.PrimaryPart.Position)
Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  if Distance < Max_Distance then
Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  Max_Distance = Distance
Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  Found_Entity = Entity
Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  end
Â Â Â Â Â Â Â Â Â Â Â  end
Â Â Â Â Â Â Â  end
Â Â Â  end
Â Â Â  
Â Â Â  Closest_Entity = Found_Entity
Â Â Â  return Found_Entity
end

function Auto_Parry:Get_Entity_Properties()
Â Â Â  Auto_Parry.Closest_Player()

Â Â Â  if not Closest_Entity then
Â Â Â Â Â Â Â  return false
Â Â Â  end

Â Â Â  local Entity_Velocity = Closest_Entity.PrimaryPart.Velocity
Â Â Â  local Entity_Direction = (Player.Character.PrimaryPart.Position - Closest_Entity.PrimaryPart.Position).Unit
Â Â Â  local Entity_Distance = (Player.Character.PrimaryPart.Position - Closest_Entity.PrimaryPart.Position).Magnitude

Â Â Â  return {
Â Â Â Â Â Â Â  Velocity = Entity_Velocity,
Â Â Â Â Â Â Â  Direction = Entity_Direction,
Â Â Â Â Â Â Â  Distance = Entity_Distance
Â Â Â  }
end

local isMobile = UserInputService.TouchEnabled and not UserInputService.MouseEnabled


function Auto_Parry.Parry_Data(Parry_Type)
Â Â Â  Auto_Parry.Closest_Player()
Â Â Â  
Â Â Â  local Events = {}
Â Â Â  local Camera = workspace.CurrentCamera
Â Â Â  local Vector2_Mouse_Location
Â Â Â  
Â Â Â  if Last_Input == Enum.UserInputType.MouseButton1 or (Enum.UserInputType.MouseButton2 or Last_Input == Enum.UserInputType.Keyboard) then
Â Â Â Â Â Â Â  local Mouse_Location = UserInputService:GetMouseLocation()
Â Â Â Â Â Â Â  Vector2_Mouse_Location = {Mouse_Location.X, Mouse_Location.Y}
Â Â Â  else
Â Â Â Â Â Â Â  Vector2_Mouse_Location = {Camera.ViewportSize.X / 2, Camera.ViewportSize.Y / 2}
Â Â Â  end
Â Â Â  
Â Â Â  if isMobile then
Â Â Â Â Â Â Â  Vector2_Mouse_Location = {Camera.ViewportSize.X / 2, Camera.ViewportSize.Y / 2}
Â Â Â  end
Â Â Â  
Â Â Â  local Players_Screen_Positions = {}
Â Â Â  for _, v in pairs(workspace.Alive:GetChildren()) do
Â Â Â Â Â Â Â  if v ~= Player.Character then
Â Â Â Â Â Â Â Â Â Â Â  local worldPos = v.PrimaryPart.Position
Â Â Â Â Â Â Â Â Â Â Â  local screenPos, isOnScreen = Camera:WorldToScreenPoint(worldPos)
Â Â Â Â Â Â Â Â Â Â Â  
Â Â Â Â Â Â Â Â Â Â Â  if isOnScreen then
Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  Players_Screen_Positions[v] = Vector2.new(screenPos.X, screenPos.Y)
Â Â Â Â Â Â Â Â Â Â Â  end
Â Â Â Â Â Â Â Â Â Â Â  
Â Â Â Â Â Â Â Â Â Â Â  Events[tostring(v)] = screenPos
Â Â Â Â Â Â Â  end
Â Â Â  end
Â Â Â  
Â Â Â  if Parry_Type == 'Camera' then
Â Â Â Â Â Â Â  return {0, Camera.CFrame, Events, Vector2_Mouse_Location}
Â Â Â  end
Â Â Â  
Â Â Â  if Parry_Type == 'Backwards' then
Â Â Â Â Â Â Â  local Backwards_Direction = Camera.CFrame.LookVector * -10000
Â Â Â Â Â Â Â  Backwards_Direction = Vector3.new(Backwards_Direction.X, 0, Backwards_Direction.Z)
Â Â Â Â Â Â Â  return {0, CFrame.new(Camera.CFrame.Position, Camera.CFrame.Position + Backwards_Direction), Events, Vector2_Mouse_Location}
Â Â Â  end

Â Â Â  if Parry_Type == 'Straight' then
Â Â Â Â Â Â Â  local Aimed_Player = nil
Â Â Â Â Â Â Â  local Closest_Distance = math.huge
Â Â Â Â Â Â Â  local Mouse_Vector = Vector2.new(Vector2_Mouse_Location[1], Vector2_Mouse_Location[2])
Â Â Â Â Â Â Â  
Â Â Â Â Â Â Â  for _, v in pairs(workspace.Alive:GetChildren()) do
Â Â Â Â Â Â Â Â Â Â Â  if v ~= Player.Character then
Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  local worldPos = v.PrimaryPart.Position
Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  local screenPos, isOnScreen = Camera:WorldToScreenPoint(worldPos)
Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  
Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  if isOnScreen then
Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  local playerScreenPos = Vector2.new(screenPos.X, screenPos.Y)
Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  local distance = (Mouse_Vector - playerScreenPos).Magnitude
Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  
Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  if distance < Closest_Distance then
Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  Closest_Distance = distance
Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  Aimed_Player = v
Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  end
Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  end
Â Â Â Â Â Â Â Â Â Â Â  end
Â Â Â Â Â Â Â  end
Â Â Â Â Â Â Â  
Â Â Â Â Â Â Â  if Aimed_Player then
Â Â Â Â Â Â Â Â Â Â Â  return {0, CFrame.new(Player.Character.PrimaryPart.Position, Aimed_Player.PrimaryPart.Position), Events, Vector2_Mouse_Location}
Â Â Â Â Â Â Â  else
Â Â Â Â Â Â Â Â Â Â Â  return {0, CFrame.new(Player.Character.PrimaryPart.Position, Closest_Entity.PrimaryPart.Position), Events, Vector2_Mouse_Location}
Â Â Â Â Â Â Â  end
Â Â Â  end
Â Â Â  
Â Â Â  if Parry_Type == 'Random' then
Â Â Â Â Â Â Â  return {0, CFrame.new(Camera.CFrame.Position, Vector3.new(math.random(-4000, 4000), math.random(-4000, 4000), math.random(-4000, 4000))), Events, Vector2_Mouse_Location}
Â Â Â  end
Â Â Â  
Â Â Â  if Parry_Type == 'High' then
Â Â Â Â Â Â Â  local High_Direction = Camera.CFrame.UpVector * 10000
Â Â Â Â Â Â Â  return {0, CFrame.new(Camera.CFrame.Position, Camera.CFrame.Position + High_Direction), Events, Vector2_Mouse_Location}
Â Â Â  end
Â Â Â  
Â Â Â  if Parry_Type == 'Left' then
Â Â Â Â Â Â Â  local Left_Direction = Camera.CFrame.RightVector * 10000
Â Â Â Â Â Â Â  return {0, CFrame.new(Camera.CFrame.Position, Camera.CFrame.Position - Left_Direction), Events, Vector2_Mouse_Location}
Â Â Â  end
Â Â Â  
Â Â Â  if Parry_Type == 'Right' then
Â Â Â Â Â Â Â  local Right_Direction = Camera.CFrame.RightVector * 10000
Â Â Â Â Â Â Â  return {0, CFrame.new(Camera.CFrame.Position, Camera.CFrame.Position + Right_Direction), Events, Vector2_Mouse_Location}
Â Â Â  end

Â Â Â  if Parry_Type == 'RandomTarget' then
Â Â Â Â Â Â Â  local candidates = {}
Â Â Â Â Â Â Â  for _, v in pairs(workspace.Alive:GetChildren()) do
Â Â Â Â Â Â Â Â Â Â Â  if v ~= Player.Character and v.PrimaryPart then
Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  local screenPos, isOnScreen = Camera:WorldToScreenPoint(v.PrimaryPart.Position)
Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  if isOnScreen then
Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  table.insert(candidates, {
Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  character = v,
Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  screenXYÂ  = { screenPos.X, screenPos.Y }
Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  })
Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  end
Â Â Â Â Â Â Â Â Â Â Â  end
Â Â Â Â Â Â Â  end
Â Â Â Â Â Â Â  if #candidates > 0 then
Â Â Â Â Â Â Â Â Â Â Â  local pick = candidates[ math.random(1, #candidates) ]
Â Â Â Â Â Â Â Â Â Â Â  local lookCFrame = CFrame.new(Player.Character.PrimaryPart.Position, pick.character.PrimaryPart.Position)
Â Â Â Â Â Â Â Â Â Â Â  return {0, lookCFrame, Events, pick.screenXY}
Â Â Â Â Â Â Â  else
Â Â Â Â Â Â Â Â Â Â Â  return {0, Camera.CFrame, Events, { Camera.ViewportSize.X/2, Camera.ViewportSize.Y/2 }}
Â Â Â Â Â Â Â  end
Â Â Â  end
Â Â Â  
Â Â Â  return Parry_Type
end

function Auto_Parry.Parry(Parry_Type)
Â Â Â  local Parry_Data = Auto_Parry.Parry_Data(Parry_Type)

Â Â Â  if not firstParryFired then
Â Â Â Â Â Â Â  performFirstPress(firstParryType)
Â Â Â Â Â Â Â  firstParryFired = true
Â Â Â  else
Â Â Â Â Â Â Â  Parry(Parry_Data[1], Parry_Data[2], Parry_Data[3], Parry_Data[4])
Â Â Â  end

Â Â Â  if Parries > 7 then
Â Â Â Â Â Â Â  return false
Â Â Â  end

Â Â Â  Parries += 1

Â Â Â  task.delay(0.5, function()
Â Â Â Â Â Â Â  if Parries > 0 then
Â Â Â Â Â Â Â Â Â Â Â  Parries -= 1
Â Â Â Â Â Â Â  end
Â Â Â  end)
end

local Lerp_Radians = 0
