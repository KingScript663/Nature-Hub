local Auto_Parry = {}

function Auto_Parry.Parry_Animation()
Â Â Â  local Parry_Animation = game:GetService("ReplicatedStorage").Shared.SwordAPI.Collection.Default:FindFirstChild('GrabParry')
Â Â Â  local Current_Sword = Player.Character:GetAttribute('CurrentlyEquippedSword')

Â Â Â  if not Current_Sword then
Â Â Â Â Â Â Â  return
Â Â Â  end

Â Â Â  if not Parry_Animation then
Â Â Â Â Â Â Â  return
Â Â Â  end

Â Â Â  local Sword_Data = game:GetService("ReplicatedStorage").Shared.ReplicatedInstances.Swords.GetSword:Invoke(Current_Sword)

Â Â Â  if not Sword_Data or not Sword_Data['AnimationType'] then
Â Â Â Â Â Â Â  return
Â Â Â  end

Â Â Â  for _, object in pairs(game:GetService('ReplicatedStorage').Shared.SwordAPI.Collection:GetChildren()) do
Â Â Â Â Â Â Â  if object.Name == Sword_Data['AnimationType'] then
Â Â Â Â Â Â Â Â Â Â Â  if object:FindFirstChild('GrabParry') or object:FindFirstChild('Grab') then
Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  local sword_animation_type = 'GrabParry'

Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  if object:FindFirstChild('Grab') then
Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  sword_animation_type = 'Grab'
Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  end

Â Â Â Â Â Â Â Â Â Â Â Â Â Â Â  Parry_Animation = object[sword_animation_type]
Â Â Â Â Â Â Â Â Â Â Â  end
Â Â Â Â Â Â Â  end
Â Â Â  end

Â Â Â  Grab_Parry = Player.Character.Humanoid.Animator:LoadAnimation(Parry_Animation)
Â Â Â  Grab_Parry:Play()
end

