function Auto_Parry.Linear_Interpolation(a, b, time_volume)
Â Â Â  return a + (b - a) * time_volume
end

local Previous_Velocity = {}
local Curving = tick()

local Runtime = workspace.Runtime


function Auto_Parry.Is_Curved()
Â Â Â  local Ball = Auto_Parry.Get_Ball()

Â Â Â  if not Ball then
Â Â Â Â Â Â Â  return false
Â Â Â  end

Â Â Â  local Zoomies = Ball:FindFirstChild('zoomies')

Â Â Â  if not Zoomies then
Â Â Â Â Â Â Â  return false
Â Â Â  end

Â Â Â  local Velocity = Zoomies.VectorVelocity
Â Â Â  local Ball_Direction = Velocity.Unit

Â Â Â  local Direction = (Player.Character.PrimaryPart.Position - Ball.Position).Unit
Â Â Â  local Dot = Direction:Dot(Ball_Direction)

Â Â Â  local Speed = Velocity.Magnitude
Â Â Â  local Speed_Threshold = math.min(Speed / 100, 40)

Â Â Â  local Direction_Difference = (Ball_Direction - Velocity).Unit
Â Â Â  local Direction_Similarity = Direction:Dot(Direction_Difference)

Â Â Â  local Dot_Difference = Dot - Direction_Similarity
Â Â Â  local Distance = (Player.Character.PrimaryPart.Position - Ball.Position).Magnitude

Â Â Â  local Pings = game:GetService('Stats').Network.ServerStatsItem['Data Ping']:GetValue()

Â Â Â  local Dot_Threshold = 0.5 - (Pings / 1000)
Â Â Â  local Reach_Time = Distance / Speed - (Pings / 1000)

Â Â Â  local Ball_Distance_Threshold = 15 - math.min(Distance / 1000, 15) + Speed_Threshold

Â Â Â  local Clamped_Dot = math.clamp(Dot, -1, 1)
Â Â Â  local Radians = math.rad(math.asin(Clamped_Dot))

Â Â Â  Lerp_Radians = Auto_Parry.Linear_Interpolation(Lerp_Radians, Radians, 0.8)

Â Â Â  if Speed > 100 and Reach_Time > Pings / 10 then
Â Â Â Â Â Â Â  Ball_Distance_Threshold = math.max(Ball_Distance_Threshold - 15, 15)
Â Â Â  end

Â Â Â  if Distance < Ball_Distance_Threshold then
Â Â Â Â Â Â Â  return false
Â Â Â  end

Â Â Â  if Dot_Difference < Dot_Threshold then
Â Â Â Â Â Â Â  return true
Â Â Â  end

Â Â Â  if Lerp_Radians < 0.018 then
Â Â Â Â Â Â Â  Last_Warping = tick()
Â Â Â  end

Â Â Â  if (tick() - Last_Warping) < (Reach_Time / 1.5) then
Â Â Â Â Â Â Â  return true
Â Â Â  end

Â Â Â  if (tick() - Curving) < (Reach_Time / 1.5) then
Â Â Â Â Â Â Â  return true
Â Â Â  end

Â Â Â  return Dot < Dot_Threshold
end

function Auto_Parry:Get_Ball_Properties()
Â Â Â  local Ball = Auto_Parry.Get_Ball()

Â Â Â  local Ball_Velocity = Vector3.zero
Â Â Â  local Ball_Origin = Ball

Â Â Â  local Ball_Direction = (Player.Character.PrimaryPart.Position - Ball_Origin.Position).Unit
Â Â Â  local Ball_Distance = (Player.Character.PrimaryPart.Position - Ball.Position).Magnitude
Â Â Â  local Ball_Dot = Ball_Direction:Dot(Ball_Velocity.Unit)

Â Â Â  return {
Â Â Â Â Â Â Â  Velocity = Ball_Velocity,
Â Â Â Â Â Â Â  Direction = Ball_Direction,
Â Â Â Â Â Â Â  Distance = Ball_Distance,
Â Â Â Â Â Â Â  Dot = Ball_Dot
Â Â Â  }
end

function Auto_Parry.Spam_Service(self)
Â Â Â  local Ball = Auto_Parry.Get_Ball()

Â Â Â  local Entity = Auto_Parry.Closest_Player()

Â Â Â  if not Ball then
Â Â Â Â Â Â Â  return false
Â Â Â  end

Â Â Â  if not Entity or not Entity.PrimaryPart then
Â Â Â Â Â Â Â  return false
Â Â Â  end

Â Â Â  local Spam_Accuracy = 0

Â Â Â  local Velocity = Ball.AssemblyLinearVelocity
Â Â Â  local Speed = Velocity.Magnitude

Â Â Â  local Direction = (Player.Character.PrimaryPart.Position - Ball.Position).Unit
Â Â Â  local Dot = Direction:Dot(Velocity.Unit)

Â Â Â  local Target_Position = Entity.PrimaryPart.Position
Â Â Â  local Target_Distance = Player:DistanceFromCharacter(Target_Position)

Â Â Â  local Maximum_Spam_Distance = self.Ping + math.min(Speed / 6, 95)

Â Â Â  if self.Entity_Properties.Distance > Maximum_Spam_Distance then
Â Â Â Â Â Â Â  return Spam_Accuracy
Â Â Â  end

Â Â Â  if self.Ball_Properties.Distance > Maximum_Spam_Distance then
Â Â Â Â Â Â Â  return Spam_Accuracy
Â Â Â  end

Â Â Â  if Target_Distance > Maximum_Spam_Distance then
Â Â Â Â Â Â Â  return Spam_Accuracy
Â Â Â  end

Â Â Â  local Maximum_Speed = 5 - math.min(Speed / 5, 5)
Â Â Â  local Maximum_Dot = math.clamp(Dot, -1, 0) * Maximum_Speed

Â Â Â  Spam_Accuracy = Maximum_Spam_Distance - Maximum_Dot

Â Â Â  return Spam_Accuracy
end

